@function dispsel()

% DISPSEL:  Selection Display window code for CSIRO CTD cast selection program.
%   To be invoked only via "ctd_select"
%
%  Copyright     Jeff Dunn, CSIRO Division of Marine Research, 29/1/1998

% $Id: $
%
% This code manually modified after creation using Matlab5 GUIDE

load dispsel                           

a = figure('Units','points', ...
	'Color',[0.8 0.8 0.8], ...
	'Colormap',mat0, ...
	'MenuBar','none', ...
	'PaperType','a4letter', ...
	'Position',[0 150 773.76 430.08]);

b = uicontrol('Parent',a, ...
	'Units','points', ...
	'BackgroundColor',[1 1 1], ...
	'Callback','dispsel_util(''one_cru'')', ...
	'Position',[20 65 115 310], ...
	'Style','listbox', ...
	'Tag','cru_list_box', ...
	'Value',1);
b = uicontrol('Parent',a, ...
	'Units','points', ...
	'BackgroundColor',[0.7 0.7 0.7], ...
	'FontSize',12, ...
	'Position',[16.32 399.36 135.36 17.28], ...
	'String',' ',...
	'Style','text',...
	'Tag','disp_title');
b = uicontrol('Parent',a, ...
	'Units','points', ...
	'Callback','dispsel_util(''continue'')', ...
	'BackgroundColor',[0 0 0.5], ...
	'FontSize',14, ...
	'ForegroundColor',[1 1 1], ...
	'Position',[41.28 10.56 75.84 37.44], ...
	'String','Continue');
b = uicontrol('Parent',a, ...
	'Units','points', ...
	'Callback','dispsel_util(''map'')', ...
	'Position',[255 10 180 17.3], ...
	'String','Cast Location Map');
b = uicontrol('Parent',a, ...
	'Units','points', ...
	'Callback','dispsel_util(''histo'')', ...
	'Position',[488 10 180 17.3], ...
	'String','Timeseries Histogram');
b = axes('Parent',a, ...
	'Units','points', ...
	'CameraUpVector',[0 1 0], ...
	'Color',[1 1 1], ...
	'ColorOrder',mat1, ...
	'Position',[169.92 44.16 581.76 373.44], ...
	'Tag','Axes1', ...
	'XColor',[0 0 0], ...
	'YColor',[0 0 0], ...
	'ZColor',[0 0 0]);
c = text('Parent',b, ...
	'Color',[0 0 0], ...
	'HandleVisibility','callback', ...
	'HorizontalAlignment','center', ...
	'Position',[0.499174 -0.0489691 0], ...
	'Tag','Axes1Text4', ...
	'VerticalAlignment','cap');
set(get(c,'Parent'),'XLabel',c);
c = text('Parent',b, ...
	'Color',[0 0 0], ...
	'HandleVisibility','callback', ...
	'HorizontalAlignment','center', ...
	'Position',[-0.0380165 0.497423 0], ...
	'Rotation',90, ...
	'Tag','Axes1Text3', ...
	'VerticalAlignment','baseline');
set(get(c,'Parent'),'YLabel',c);
c = text('Parent',b, ...
	'Color',[0 0 0], ...
	'HandleVisibility','callback', ...
	'HorizontalAlignment','right', ...
	'Position',[-0.294215 1.03093 0], ...
	'Tag','Axes1Text2');
set(get(c,'Parent'),'Title',c);

dispsel_util('init')

